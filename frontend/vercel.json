{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "nodeVersion": "18.x",
  "env": {
    "REACT_APP_API_URL": "https://nutrilea-backend.onrender.com/api"
  }
}
